depend() {
	need localmount
	before net
}

start() {
	ebegin "Starting Dynamic Kernel Module Support "
		/usr/lib/dkms/dkms_autoinstaller start
	eend 0
}

status() {
	/usr/lib/dkms/dkms_autoinstaller status
}
